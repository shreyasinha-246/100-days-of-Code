Q133: Create an enum for months and print how many days each month has.

/*
Sample Test Cases:
Input 1:
FEB
Output 1:
28 or 29 days

*/
#include <stdio.h>

int main(){

    enum months {JAN, FEB, MAR, APR, MAY, JUN, JUL, AUG, SEP, OCT, NOV, DEC};

    int m;
    printf("enter month number (0=JAN ... 11=DEC): ");
    scanf("%d",&m);

    enum months mon = m;

    if(mon == JAN) printf("31 days\n");
    else if(mon == FEB) printf("28 or 29 days\n");
    else if(mon == MAR) printf("31 days\n");
    else if(mon == APR) printf("30 days\n");
    else if(mon == MAY) printf("31 days\n");
    else if(mon == JUN) printf("30 days\n");
    else if(mon == JUL) printf("31 days\n");
    else if(mon == AUG) printf("31 days\n");
    else if(mon == SEP) printf("30 days\n");
    else if(mon == OCT) printf("31 days\n");
    else if(mon == NOV) printf("30 days\n");
    else if(mon == DEC) printf("31 days\n");
    else printf("wrong month number...\n");

    return 0;
}